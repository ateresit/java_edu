<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.itia.cs.client.Controller"
            prefHeight="400.0" prefWidth="600.0">

<VBox>
    <TextArea fx:id="mainWindow" editable="false" VBox.vgrow="ALWAYS"/>
    <HBox>
        <TextField fx:id="commandLine" HBox.hgrow="ALWAYS" promptText="Введите команду здесь: "/>
        <Button text="send" onAction="#sendMsgAction"/>
    </HBox>
</VBox>

</AnchorPane>
